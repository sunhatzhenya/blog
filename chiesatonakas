        {/block:Tags}
        </div>
        {/block:HasTags}
            </div>
        {/block:ifShowTags}
        </div>
   
    {block:PermalinkPage}
    {block:RebloggedFrom}
        <div class="post_source">
    <a href="{ReblogParentURL}" title="{ReblogParentTitle}">VIA {ReblogParentName}</a>
    <a href="{ReblogRootURL}" title="{ReblogRootTitle}">SOURCE {ReblogRootName}</a>
        </div>
    {/block:RebloggedFrom}
    {/block:PermalinkPage}
   
    {block:PermalinkPage}
    {block:PostNotes}
    <div class="postnotes">
        {PostNotes}
    </div>
    {/block:PostNotes}
    {/block:PermalinkPage}
   
    {/block:Posts}
    </div>
    </div>
 
</div>
   
   
